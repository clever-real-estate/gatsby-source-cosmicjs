{
  "name": "@cleverrealestate/gatsby-source-cosmicjs",
  "version": "0.0.1",
  "description": "Gatsby Source Plugin for building websites using Cosmic JS as a data source with a lil Clever flair",
  "author": {
    "name": "Clever",
    "url": "https://cosmicjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmicjs/gatsby-source-cosmicjs.git"
  },
  "bugs": {
    "url": "https://github.com/cosmicjs/gatsby-source-cosmicjs/issues"
  },
  "license": "MIT",
  "homepage": "https://cosmicjs.com",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write 'src/**/*.js'",
    "watch": "babel -w src --out-dir ."
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "cosmicjs"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "gatsby-node-helpers": "^0.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "query-string": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "babel-preset-gatsby-package": "^0.2.16",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.13.26"
  },
  "pre-commit": [
    "format"
  ]
}
